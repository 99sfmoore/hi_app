<div class="container">
  <div id="rect-demo">
  </div>
  <div id="bubble">
  </div>
</div>

<script>
  var rectDemo = d3.select("#rect-demo").
    append("svg:svg").
    attr("width", 400).
    attr("height", 300);

  rectDemo.append("svg:rect").
    attr("x", 100).
    attr("y", 100).
    attr("height", 100).
    attr("width", 200);
</script>

<script type="text/javascript">

    var diameter = 960,
            format = d3.format(",d"),
            color = d3.scale.category20c();

    var bubble = d3.layout.pack()
            .sort(null)
            .size([diameter, diameter])
            .padding(1.5);

    var svg = d3.select("#bubble").append("svg")
            .attr("width", diameter)
            .attr("height", diameter)
            .attr("class", "bubble");
</script>