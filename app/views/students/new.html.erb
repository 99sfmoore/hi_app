% provide(:title, 'New Student') %>
<h1 class=""> Create new student</h1>
<div class="row">
  <div class="col-sm-5 col-sm-offset-1">
    <%= form_for @student do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Add student", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>